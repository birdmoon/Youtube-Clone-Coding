extends layouts/main
include mixins/videoBlock

block content
    .videos
        each item in videos2 
            +videoBlock({
                id:item.id,
                title:item.title,
                views:item.views,
                videoFile:item.videoFile
            })
//- //item은 변수명이고 어떤 이름을 붙여도 상관없다.
//- videos2는 db.js에 불러온 가상 DB 배열이다.
        
